Create a store

<.form let={f} for={@store_changeset} phx-submit="create_store">
  <%= if @store_changeset.action do %>
    <div class="alert alert-danger">
      <p>Oops, something went wrong! Please check the errors below.</p>
    </div>
  <% end %>

  <%= error_tag f, :label %>
  <%= label f, :label, "Label" %>
  <%= text_input f, :label %>

  <div>
    <%= submit "Create" %>
  </div>
</.form>

<button phx-click="skip" class="skip">Skip</button>
