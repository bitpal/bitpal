Stores:

<section class="stores">
  <ul>
    <%= for {_id, store} <- @stores do %>
      <li><%= live_redirect(store.label, to: Routes.store_path(@socket, :show, store)) %></li>
    <% end %>
  </ul>
</section>

<%= link("Server settings", to: Routes.server_settings_path(@socket, :show)) %>

<section class="backend_status">
  <%= for status <- @backend_status do %>
    <%= format_status(status) %>
  <% end %>
</section>

